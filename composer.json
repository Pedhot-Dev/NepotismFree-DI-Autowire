{
    "name": "pedhot-dev/nepotismfree-autowire",
    "description": "An optional Autowire / Resolver layer for NepotismFree-DI. Strict DI stays strict; this resolver sits on top.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Pedhot-Dev",
            "homepage": "https://github.com/Pedhot-Dev"
        }
    ],
    "support": {
        "source": "https://github.com/Pedhot-Dev/nepotismfree-autowire",
        "issues": "https://github.com/Pedhot-Dev/nepotismfree-autowire/issues"
    },
    "require": {
        "php": "^8.1",
        "pedhot-dev/nepotismfree-di": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "PedhotDev\\NepotismFree\\Autowire\\": "src/Autowire/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "virion": {
            "spec": "3.1",
            "namespace-root": "PedhotDev\\NepotismFree\\Autowire"
        }
    }
}